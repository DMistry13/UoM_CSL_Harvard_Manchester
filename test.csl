<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" and="symbol" et-al-use-first="3" demote-non-dropping-particle="never">
  <info>
    <title>Harvard Manchester</title>
    <id>http://www.zotero.org/styles/harvard-manchester</id>
    <link rel="self" href="http://www.zotero.org/styles/harvard-manchester"/>
    <author>
      <name>Adrian Kosikowski</name>
    </author>
    <author>
      <name>Preenal Asher</name>
    </author>
    <author>
      <name>Dhruv Mistry</name>
    </author>
    <updated>2024-11-28T14:25:59+00:00</updated>
  </info>
  <macro name="author">
    <choose>
      <if match="all" variable="author editor">
        <names variable="author" suffix=" ">
          <name delimiter="" suffix=" " delimiter-precedes-last="always" et-al-min="4" initialize-with="." name-as-sort-order="first"/>
        </names>
      </if>
      <else-if match="any" variable="editor">
        <names variable="editor">
          <name and="symbol" delimiter-precedes-last="always" et-al-min="4" initialize-with="." name-as-sort-order="all"/>
        </names>
      </else-if>
      <else>
        <names variable="author editor guest">
          <name and="symbol" delimiter-precedes-last="never" et-al-min="4" initialize-with="." name-as-sort-order="all"/>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="year">
    <group>
      <date date-parts="year" form="text" variable="issued" prefix="(" suffix=")."/>
    </group>
  </macro>
  <macro name="specific title">
    <group>
      <text variable="title" prefix=" '" suffix="', "/>
    </group>
  </macro>
  <macro name="title">
    <group>
      <text variable="container-title" font-style="italic"/>
    </group>
    <choose>
      <if match="any" variable="collection-title">
        <text variable="collection-title" font-style="italic" prefix=": " suffix=". "/>
      </if>
      <else>
        <text value="." suffix=". "/>
      </else>
    </choose>
  </macro>
  <macro name="editions">
    <group delimiter=" ">
      <number variable="edition" form="ordinal"/>
      <text value="edn. "/>
    </group>
  </macro>
  <macro name="medium">
    <group prefix="[" suffix="]. ">
      <text variable="medium"/>
    </group>
  </macro>
  <macro name="publisher">
    <group delimiter=" ">
      <text variable="publisher-place" suffix=":"/>
      <text variable="publisher" suffix="."/>
    </group>
  </macro>
  <macro name="last update">
    <choose>
      <if type="post" match="any">
        <date variable="issued">
          <date-part name="hour" suffix=":"/>
          <date-part name="minute" suffix=", "/>
        </date>
      </if>
    </choose>
    <date variable="submitted">
      <date-part name="day" suffix=" "/>
      <date-part name="month" form="long" suffix=" "/>
      <date-part name="year" suffic=". "/>
    </date>
  </macro>
  <macro name="page number">
    <group>
      <label variable="page" form="short"/>
      <text variable="page"/>
    </group>
  </macro>
  <macro name="access">
    <group delimiter=" ">
      <choose>
        <if match="any" variable="URL">
          <text value="Available at:"/>
          <text variable="URL" suffix="."/>
        </if>
        <else-if match="any" variable="DOI">
          <text variable="DOI"/>
        </else-if>
      </choose>
      <group prefix=" (" suffix=").">
        <text value="Accessed: "/>
        <date variable="accessed">
          <date-part name="day" form="numeric-leading-zeros" suffix="."/>
          <date-part name="month" form="numeric-leading-zeros" suffix="."/>
          <date-part name="year"/>
        </date>
      </group>
    </group>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" collapse="year" delimiter="; ">
    <sort>
      <key macro="author"/>
      <key macro="issued"/>
    </sort>
    <layout delimiter=", ">
      <choose>
        <if match="any" type="article">
          <text macro="author" suffix=" "/>
          <date variable="issued" prefix="(" suffix=")">
            <date-part name="year"/>
          </date>
        </if>
        <else>
          <text variable="title" font-style="italic" prefix="(" suffix=", "/>
          <date variable="issued" suffix=")">
            <date-part name="year"/>
          </date>
        </else>
      </choose>
    </layout>
  </citation>
  <bibliography hanging-indent="true" line-spacing="1" entry-spacing="0">
    <layout delimiter=". ">
      <group delimiter=" ">
        <choose>
          <if match="any" type="broadcast">
            <group delimiter=" ">
              <text macro="title"/>
              <text macro="year"/>
              <names variable="director">
                <name suffix=" " delimiter-precedes-last="always" et-al-min="4"/>
              </names>
              <text macro="medium"/>
              <text macro="publisher"/>
            </group>
          </if>
          <else-if type="collection" match="any">
            <group delimiter=" ">
              <names variable="composer">
                <name delimiter-precedes-last="always" et-al-min="3" initialize-with="." name-as-sort-order="all"/>
              </names>
              <text macro="year"/>
              <text macro="specific title"/>
              <text macro="title"/>
              <text macro="medium"/>
              <text macro="publisher"/>
            </group>
          </else-if>
          <else-if type="legislation" match="any">
            <group delimiter=" ">
              <text macro="title"/>
            </group>
          </else-if>
          <else-if type="post-weblog" match="any">
            <group delimiter=" ">
              <text macro="author"/>
              <text macro="specific title"/>
              <text variable="original-title" suffix=","/>
              <text macro="last update"/>
            </group>
          </else-if>
          <else-if type="speech" match="any">
            <group delimiter=" ">
              <names variable="author" suffix=",">
                <name name-as-sort-order="all"/>
              </names>
              <text macro="year"/>
              <text macro="title"/>
              <text macro="medium"/>
              <text term="title-locator"/>
              <text value="institution" suffix="Status: unpublished."/>
            </group>
          </else-if>
          <else-if type="standard" match="any">
            <text value="institution"/>
            <text macro="year"/>
            <number suffix=":" variable="number"/>
            <text macro="title"/>
            <text macro="publisher"/>
            <text macro="medium"/>
          </else-if>
          <else>
            <choose>
              <if match="any" variable="author editor" type="interview">
                <text macro="author"/>
              </if>
              <else>
                <text macro="title"/>
              </else>
            </choose>
            <text macro="year"/>
          </else>
        </choose>
        <choose>
          <if type="article-journal">
            <group delimiter=" ">
              <text macro="specific title"/>
              <text macro="title" font-style="italic"/>
              <group delimiter=" " suffix=".">
                <text variable="volume" suffix=":"/>
                <choose>
                  <if match="any" variable="issue">
                    <text variable="issue" prefix="(" suffix=")"/>
                  </if>
                  <else-if match="any" variable="part-number">
                    <text variable="part-number" prefix="(" suffix=")"/>
                  </else-if>
                </choose>
              </group>
              <text macro="page number"/>
              <text variable="source" suffix=" [Preprint] "/>
            </group>
          </if>
          <else-if type="article-newspaper" match="any">
            <text macro="specific title"/>
            <choose>
              <if match="any" variable="author editor">
                <text macro="title"/>
              </if>
              <else>
                <text value=" "/>
              </else>
            </choose>
            <text macro="last update"/>
          </else-if>
          <else-if type="book" match="any">
            <text variable="title" font-style="italic" suffix="."/>
            <choose>
              <if match="any" variable="collection-title">
                <text variable="collection-title"/>
              </if>
              <else/>
            </choose>
            <choose>
              <if match="all" variable="author editor">
                <names variable="editor" prefix="Edited by " suffix=".">
                  <name and="symbol" et-al-use-last="true" initialize-with="."/>
                </names>
              </if>
              <else-if match="any" variable="editor-translator">
                <names variable="editor-translator" prefix="Translated by ">
                  <name delimiter-precedes-last="always" et-al-min="4" initialize-with=". " name-as-sort-order="all"/>
                </names>
              </else-if>
            </choose>
            <text macro="editions"/>
            <text macro="publisher"/>
            <text variable="source" font-style="italic" suffix="."/>
          </else-if>
          <else-if type="chapter">
            <text macro="specific title"/>
            <names variable="editor" prefix="in " suffix=".">
              <name et-al-min="4" initialize-with=". " name-as-sort-order="first"/>
            </names>
            <text macro="title"/>
            <text macro="publisher"/>
            <text macro="page number"/>
          </else-if>
          <else-if type="dataset" match="any"/>
          <else-if type="document" match="any"/>
          <else-if type="entry-encyclopedia" match="any">
            <text macro="title"/>
          </else-if>
          <else-if type="figure" match="any">
            <text macro="title"/>
            <text variable="source" prefix="[" suffix="]"/>
          </else-if>
          <else-if type="graphic" match="any">
            <text macro="title"/>
            <text macro="medium"/>
            <text macro="publisher"/>
            <text variable="publisher" suffix=":"/>
            <text variable="publisher-place" prefix="Publisher: " suffix="."/>
            <text macro="page number"/>
          </else-if>
          <else-if type="interview" match="any">
            <text variable="container-title" prefix="'" suffix="'."/>
            <text term="editortranslator"/>
            <choose>
              <if match="all" variable="interviewer page">
                <text variable="container-title" prefix="'" suffix="'."/>
                <names variable="guest" prefix="Interview with ">
                  <name delimiter-precedes-last="always" et-al-min="3" initialize-with="." name-as-sort-order="first"/>
                </names>
                <names variable="interviewer" prefix="Interviewed by ">
                  <name delimiter-precedes-last="always" et-al-min="3" initialize-with="." name-as-sort-order="first"/>
                </names>
                <text variable="publisher" font-style="italic" prefix="for the "/>
                <choose>
                  <if match="any" variable="page">
                    <text macro="page number"/>
                    <date form="text" variable="event-date">
                      <date-part name="day"/>
                      <date-part name="month"/>
                    </date>
                  </if>
                  <else>
                    <date form="text" date-parts="year-month-day" variable="issued">
                      <date-part name="day"/>
                      <date-part name="month"/>
                    </date>
                  </else>
                </choose>
              </if>
              <else>
                <text macro="specific title"/>
                <text macro="medium"/>
                <text variable="publisher-place"/>
                <text variable="status" prefix="&quot;" suffix="&quot;"/>
              </else>
            </choose>
          </else-if>
          <else-if type="legal_case" match="any">
            <text variable="authority" suffix=","/>
            <number/>
            <text variable="call-number" prefix="Case: " suffix="."/>
            <text variable="source" font-style="italic" suffix="."/>
          </else-if>
          <else-if type="map" match="any">
            <choose>
              <if match="any" variable="URL DOI">
                <text variable="container-title" font-style="italic" suffix=":"/>
                <text variable="title"/>
                <text variable="page" suffix="."/>
                <text variable="scale" suffix="."/>
                <text variable="source" prefix="Using: "/>
                <text variable="publisher-place" suffix="."/>
              </if>
              <else>
                <text variable="container-title" font-style="italic" suffix="."/>
                <text variable="scale" prefix="Scale "/>
                <text macro="title"/>
                <text macro="publisher"/>
              </else>
            </choose>
          </else-if>
          <else-if type="motion_picture" match="any">
            <text variable="original-title" font-style="italic"/>
            <text term="timestamp"/>
            <date form="text" variable="issued" suffix="."/>
          </else-if>
          <else-if type="pamphlet" match="any">
            <text variable="original-title" font-style="italic" suffix=","/>
            <text macro="last update" suffix=","/>
            <text macro="medium"/>
            <text variable="event-place" suffix=":"/>
            <text variable="publisher-place" suffix="."/>
          </else-if>
          <else-if type="paper-conference" match="any">
            <text macro="specific title"/>
            <text variable="original-title" font-style="italic" suffix=","/>
            <text variable="publisher-place" suffix="."/>
            <text macro="last update"/>
            <text variable="publisher" suffix="."/>
            <text macro="page number"/>
          </else-if>
          <else-if type="patent" match="any">
            <text variable="title" font-style="italic" suffix="."/>
            <text variable="number" prefix="Patent no. " suffix="."/>
          </else-if>
          <else-if type="post" match="any">
            <text variable="title" font-style="italic"/>
            <text macro="medium"/>
            <text term="timestamp"/>
            <text macro="last update"/>
          </else-if>
          <else-if type="report" match="any">
            <choose>
              <if match="any" variable="URL DOI">
                <choose>
                  <if match="any" variable="publisher">
                    <text macro="title"/>
                    <text macro="publisher"/>
                    <number prefix="(" suffix=")." variable="supplement-number"/>
                  </if>
                  <else>
                    <text macro="title"/>
                    <names variable="organizer"/>
                  </else>
                </choose>
              </if>
              <else>
                <text macro="title"/>
                <text variable="medium" prefix="[" suffix="]"/>
                <number vertical-align="baseline" prefix="(" suffix=")" variable="supplement-number"/>
              </else>
            </choose>
          </else-if>
          <else-if type="review" match="any">
            <text macro="specific title"/>
            <text variable="source" font-style="italic"/>
            <text value="[Systematic Review]."/>
            <number prefix="Issue Number: (" variable="issue"/>
            <number prefix="/" suffix=")," variable="part-number"/>
            <number suffix="." variable="number"/>
          </else-if>
          <else-if type="software" match="any">
            <text macro="title"/>
            <text macro="medium"/>
          </else-if>
          <else-if type="song" match="any">
            <text variable="original-title" font-style="italic"/>
            <text value="[Podcast]."/>
            <text macro="last update"/>
          </else-if>
          <else-if type="thesis" match="any">
            <text variable="title-short" font-style="italic" suffix=":"/>
            <text macro="title"/>
            <text variable="genre" suffix="."/>
            <text variable="publisher" suffix="."/>
          </else-if>
          <else-if type="webpage" match="any">
            <text macro="title"/>
          </else-if>
        </choose>
      </group>
      <choose>
        <if variable="URL DOI" match="any">
          <text macro="access" prefix=" "/>
        </if>
      </choose>
    </layout>
  </bibliography>
</style>
